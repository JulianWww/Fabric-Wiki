{
  "schemaVersion": 1,
  "id": "wiki",
  "version": "${version}",

  "name": "Ingame Wiki",
  "description": "Ingame Wiki pages for Minecraft mods",
  "authors": [
    "Denanu"
  ],
  "contact": {
    "issues": "https://github.com/JulianWww/Fabric-Wiki/issues",
    "sources": "https://github.com/JulianWww/Fabric-Wiki"
  },

  "license": "CC0-1.0",
  "icon": "assets/wiki/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.denanu.wiki.WikiClient"
    ],
    "main":[
      "net.denanu.wiki.Wiki"
    ],
    "modmenu": [
      "net.denanu.wiki.compat.modmenu.Config"
    ]
  },
  "mixins": [
    "wiki.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric-api": "*",
    "minecraft": "~1.19.4",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {
    "modmenu": {
        "badges": [ "library" ],
        "update_checker": false
    },
    "wiki": {
    	"root": "root"
    }
  }
}
